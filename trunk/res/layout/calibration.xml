<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/background_ray">


	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="fill_parent" android:layout_gravity="fill_vertical"
		android:layout_weight="1" android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content"
			android:layout_gravity="center_horizontal" android:paddingTop="5dp"
			android:paddingBottom="10dp" android:cursorVisible="true"
			android:layout_width="fill_parent" android:id="@+id/settingsTitle"
			android:text="Calibration" android:paddingLeft="15dp"
			android:shadowDx="-1.0" android:shadowDy="-1.0" android:shadowRadius="0.3"
			android:textColor="#FFFFFF" android:shadowColor="#09586d"
			android:textSize="42sp"></TextView>




	</LinearLayout>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Start Calibration"
		android:id="@+id/startCalibration" android:textSize="32sp" android:layout_gravity="top|center_horizontal|center" android:padding="15dp" android:background="#3eed27"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_gravity="top|center_horizontal|center" android:id="@+id/stopCalibration"
		android:text="Stop" android:textSize="22sp" android:background="#f65959" android:padding="5dp" android:layout_marginTop="15dp"></Button>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFFFFF"
		android:textSize="24sp" android:id="@+id/max_label" android:text="maximum"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFFFFF"
		android:textSize="24sp" android:text="minimum" android:id="@+id/min_label"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#FFFFFF"
		android:textSize="24sp" android:id="@+id/d_label" android:text="Delta"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/z_label"
		android:textColor="#FFFFFF" android:textSize="24sp" android:text="label z"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/y_label"
		android:textColor="#FFFFFF" android:textSize="24sp" android:text="label y"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/x_label"
		android:textColor="#FFFFFF" android:textSize="24sp" android:text="label x"></TextView>
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center_horizontal|center" android:layout_marginBottom="5dp">
		<elements.IntentButton android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:id="@+id/backButton" android:background="@drawable/back" android:layout_gravity="center" android:gravity="center_horizontal|center"></elements.IntentButton>
	</LinearLayout>




</LinearLayout>
