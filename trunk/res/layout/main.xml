<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/background_ray">
	<TextView android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal" android:paddingTop="5dp"
		android:paddingBottom="10dp" android:cursorVisible="true"
		android:layout_width="fill_parent" android:id="@+id/settingsTitle"
		android:text="Early Bird" android:paddingLeft="15dp" android:shadowDx="-1.0"
		android:shadowDy="-1.0" android:shadowRadius="0.3" android:textColor="#FFFFFF"
		android:shadowColor="#09586d" android:textSize="42sp"></TextView>
	<elements.IntentButton android:layout_height="wrap_content"
		android:layout_gravity="center" android:typeface="normal" android:textStyle="bold" android:textSize="30sp"
		android:layout_width="250dp" android:padding="20dp"
		android:layout_marginTop="25dp" android:background="@drawable/button"
		android:text="Set Alarm" android:id="@+id/alarmNavButton">
	</elements.IntentButton>
	<elements.IntentButton android:layout_height="wrap_content"
		android:text="settings" android:shadowDx="-1.0" android:shadowDy="-1.0"
		android:shadowRadius="0.3" android:textColor="#FFFFFF"
		android:shadowColor="#09586d" android:layout_gravity="center"
		android:layout_width="fill_parent" android:textSize="28sp"
		android:id="@+id/settingsNavButton" android:background="@android:color/transparent" android:layout_marginBottom="20dp" android:layout_marginTop="20dp"></elements.IntentButton>
	<elements.IntentButton android:layout_height="wrap_content"
		android:shadowDx="-1.0" android:shadowDy="-1.0" android:shadowRadius="0.3"
		android:textColor="#FFFFFF" android:shadowColor="#09586d"
		android:layout_gravity="center" android:layout_width="fill_parent"
		android:textSize="28sp" android:id="@+id/calendarNavButton"
		android:text="calendar" android:background="@android:color/transparent" android:layout_marginBottom="20dp" android:layout_marginTop="20dp"></elements.IntentButton>
	<elements.IntentButton android:layout_height="wrap_content"
		android:shadowDx="-1.0" android:shadowDy="-1.0" android:shadowRadius="0.3"
		android:textColor="#FFFFFF" android:shadowColor="#09586d"
		android:layout_gravity="center" android:layout_width="fill_parent"
		android:textSize="28sp" android:id="@+id/testNavButton" android:text="test alarm"
		android:background="@android:color/transparent" android:layout_marginBottom="20dp" android:layout_marginTop="20dp"></elements.IntentButton>
	<elements.IntentButton android:layout_height="wrap_content"
		android:shadowDx="-1.0" android:shadowDy="-1.0" android:shadowRadius="0.3"
		android:textColor="#FFFFFF" android:shadowColor="#09586d"
		android:layout_gravity="center" android:layout_width="fill_parent"
		android:textSize="28sp" android:id="@+id/tipsNavButton" android:text="tips &amp; tricks"
		android:layout_marginBottom="25dp" android:layout_marginTop="20dp"
		android:background="@android:color/transparent"></elements.IntentButton>
</LinearLayout>
